#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

char* readline();
char* ltrim(char*);
char* rtrim(char*);

int parse_int(char*);



int main(){
    int N; 
    scanf("%d",&N);
    if ((N % 2) != 0) {
        printf("Weird\n");
    } else if (N >= 2 && N <= 5) {
        printf("Not Weird\n");
    } else if (N >= 6 && N <= 20) {
        printf("Weird\n");
    } else {
        printf("Not Weird\n");
    }
    return 0;
}
